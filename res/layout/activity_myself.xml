                        <ImageView
                            android:id="@+id/imageView_icon"
                            android:layout_width="35dp"
                            android:layout_height="35dp"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginBottom="9dip"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="9dip"
                            android:src="@drawable/myself_setting" />

                        <ImageView
                            android:id="@+id/imageView_jiantou"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="15dip"
                            android:src="@drawable/img_jiantou_xiao" />

                        <TextView
                            android:id="@+id/textView_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dip"
                            android:layout_toRightOf="@+id/imageView_icon"
                            android:maxEms="8"
                            android:text="设置"
                            android:textColor="@color/textcolor"
                            android:textSize="@dimen/title_siae" />
                    </RelativeLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="1dip"
                        android:background="@drawable/logo_xian" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>